<form method="post" action="{{ path('app_etudiant_delete', {'id': etudiant.id}) }}" onsubmit="return confirm(' Êtes vous sûr de vouloir supprimer cet etudiant?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ etudiant.id) }}">
    <button class=" btnpage-title-icon bg-danger text-white me-2 tail" style="border: none;">
        <i class="fa-solid fa-trash"></i>
    </button>
</form>
